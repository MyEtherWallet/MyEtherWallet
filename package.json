{
  "name": "myetherwallet",
  "version": "5.0.0-alpha.4",
  "description": "Client side ethereum wallet",
  "scripts": {
    "build": "npm run update && vue-cli-service build",
    "test": "jest --clearCache && npm run test:unit",
    "lint": "npm run lint:styles && npm run lint:code && npm run lint:md",
    "deploy": "gh-pages -d dist -b gh-pages",
    "build:mewcx": "npm run update && BUILD_TYPE=mewcx vue-cli-service build",
    "codecov": "npm run test:unit && codecov",
    "dev": "vue-cli-service serve",
    "lint:code": "vue-cli-service lint",
    "lint:md": "remark . -o -f",
    "lint:styles": "prettier --write 'src/**/*.scss' && stylelint 'src/**/*.scss' --fix",
    "lint:tests": "eslint --fix tests/",
    "start": "npm run update && npm run dev",
    "test:unit": "vue-cli-service test:unit",
    "update:packages": "node package-test.js",
    "update:lists": "node fetchEthListData.js",
    "update": "npm run update:packages && npm run update:lists"
  },
  "gitHooks": {
    "pre-commit": "npm run lint && npm run test"
  },
  "dependencies": {
    "idna-uts46": "1.1.0",
    "register-service-worker": "1.5.2",
