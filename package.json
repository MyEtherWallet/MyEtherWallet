{
  "name": "myetherwallet",
  "version": "5.0.5",
  "description": "Client side ethereum wallet",
  "scripts": {
    "dev": "vue-cli-service serve",
    "start": "npm run update:lists && npm run dev",
    "test:unit": "vue-cli-service test:unit",
    "test": "npm run test:unit",
    "lint:styles": "prettier --write 'src/**/*.scss' && stylelint 'src/**/*.scss' --fix",
    "lint:code": "vue-cli-service lint --fix",
    "lint:md": "remark . -o -f",
    "lint": "npm run lint:styles && npm run lint:code && npm run lint:md",
    "build": "npm run update:lists && vue-cli-service build",
    "build:mewcx": "npm run update:lists && BUILD_TYPE=MEWCX vue-cli-service build",
    "deploy": "gh-pages -d dist -b gh-pages",
    "codecov": "npm run test:unit && codecov",
    "update:lists": "node fetchEthListData.js"
  },
  "gitHooks": {
    "pre-commit": "npm run lint && npm run test"
  },
  "dependencies": {
    "idna-uts46": "^1.1.0",
    "vue": "^2.5.17",
