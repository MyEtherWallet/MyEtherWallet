    "uglify-es": "3.3.9",
    "uuid": "8.3.0",
    "vue-cli-plugin-vuetify": "2.0.7",
    "vue-i18n": "8.21.0",
    "vue-template-compiler": "2.6.12",
    "vuetify": "2.3.8",
    "wallet-address-validator": "0.2.4",
    "web3": "1.2.11",
