    "handlebars": "4.5.3",
    "hoek": "4.2.0",
    "mem": "4.0.0",
    "marked": "0.6.2",
    "lodash": "4.17.19"
  },
  "license": "MIT",
  "repository": "https://github.com/myetherwallet/myetherwallet"
}