  },
  "engines": {
    "node": ">= 10.0.0 < 12.0.0",
    "npm": ">= 6.0.0"
  },
  "greenkeeper": {
    "ignore": [
      "graphql",
      "ethereum-ens",
      "multicoin-address-validator",
      "@xkeshi/vue-qrcode",
      "vee-validate",
