  "engines": {
    "pnpm": ">=9.14.4",
    "node": ">=v22.11.0"
  },
  "packageManager": "pnpm@9.14.4",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "test:unit": "vitest",
    "test:e2e": "nightwatch --config ./nightwatch.conf.cjs tests/e2e/*",
    "build:preview": "vite preview",
    "build:only": "vite build",
    "build:watchonly": "vite build --watch",
    "type-check": "vue-tsc -p tsconfig.app.json",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepare": "husky",
    "zip": "cd dist; zip -r release.zip *;",
    "update-api-schema": "npx openapi-typescript schema.yaml -o src/mew_api/schema.ts"
