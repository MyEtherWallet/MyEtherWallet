{
  "name": "myetherwallet",
  "version": "6.7.10-hotfix.1",
  "description": "Client side ethereum wallet",
  "scripts": {
    "build": "yarn run update && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --logHeapUsage",
    "test:e2e": "vue-cli-service test:e2e --skiptags offline",
    "test:e2e-offline": "BUILD=offline vue-cli-service test:e2e --tag offline",
    "lint": "yarn run lint:code && yarn run lint:md",
    "apollo": "apollo",
    "apollo:aave": "apollo service:download -c=apolloAave.config.js src/dapps/aave-dapp/apollo/schemas/api.json && apollo client:codegen -c=apolloAave.config.js src/dapps/aave-dapp/apollo/global/globalTypes.json --target=json",
    "apollo:codegen": "yarn run apollo:schema && apollo client:codegen src/apollo/global/globalTypes.json --target=json",
    "apollo:schema": "apollo service:download src/apollo/schemas/api.json",
    "build:hash": "ROUTER_MODE=hash yarn run build",
    "build:nohash": "WEBPACK_INTEGRITY=false yarn run build:hash",
    "build:history": "ROUTER_MODE=history yarn run build",
    "build:history:sourcemaps": "ROUTER_MODE=history FULL_SOURCEMAPS=true yarn run build",
    "build:offline": "WEBPACK_INTEGRITY=false BUILD=offline yarn run build:hash",
    "codecov": "yarn run test && codecov",
    "dev": "yarn run update && yarn run juststart",
    "juststart": "vue-cli-service serve --watch",
    "juststart:offline": "ROUTER_MODE=hash BUILD=offline vue-cli-service serve --watch",
    "lint:code": "vue-cli-service lint --fix",
    "lint:md": "remark . -o",
    "lint:styles": "prettier --write 'src/**/*.{scss,css}' && stylelint 'src/**/*.{scss,css}' --fix",
    "lint:tests": "eslint --fix tests/",
    "preinstall": "find node_modules -name \".git\" -exec rm -rf {} \\; || true",
    "start": "yarn run update:mainlists && yarn run dev",
    "start:history": "yarn run update:mainlists && ROUTER_MODE=history yarn run dev",
    "start:offline": "yarn run update:mainlists && ROUTER_MODE=hash BUILD=offline yarn run dev",
    "test": "yarn run update:lists && jest --clearCache && yarn run test:unit",
    "update": "yarn run update:packages && yarn run update:lists",
    "update:lists": "node fetchLists/index.js",
    "update:mainlists": "node fetchLists/fetchMainLists.js",
    "update:packages": "node package-test.js"
  },
